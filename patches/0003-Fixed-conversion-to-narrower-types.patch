             }else {
+                if ((a < Integer.MIN_VALUE) || (a > Integer.MAX_VALUE) || (b < Integer.MIN_VALUE) || (b > Integer.MAX_VALUE) ) {
+                    throw new ArithmeticException("Value is out of range");
+                }
                 int d = (int) a, e = (int) b, f = 0;
                 f = d / e;
                 if (Double.isNaN(f)) {
